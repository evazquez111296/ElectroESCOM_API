<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.electroescom.models.pojos.modelos.Usuario" table="Usuario" >
        <id name="nombreUsuario" column="nombreUsuario"/>
        <property name="contrasenia" column="contrasenia"/>
        <property name="tipo_usuario" column="tipo_usuario"/>
        <property name="nombrePila" column="nombrePila"/>
        <property name="apellidoPaterno" column="apPaterno"/>
        <property name="apellidoMaterno" column="apMaterno"/>
        <property name="correo" column="correo"/>
        <property name="FechaNac" column="fechaNac"/>
        <property name="telefono" column="telefono"/>
        <property name="Fecha_Ingreso" column="Fecha_Ingreso"/>
    </class>
</hibernate-mapping>